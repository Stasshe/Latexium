{
  "name": "latexium",
  "version": "0.1.0",
  "type": "module",
  "description": "A powerful TypeScript library for parsing and analyzing LaTeX mathematical expressions",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "dev": "tsc --watch",
    "test": "node tests/master.test.mjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rm -rf dist",
    "prepare": "husky"
  },
  "keywords": [
    "latex",
    "math",
    "parser",
    "ast",
    "calculus",
    "symbolic",
    "mathematics",
    "cas",
    "typescript"
  ],
  "author": "Stasshe",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Stasshe/Latexium"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@types/jest": "^29.5.12",
    "@types/node": "24.5.1",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "eslint": "9.35.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "16.1.6",
    "prettier": "3.6.2",
    "rollup": "^4.21.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.2.5",
    "typescript": "5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
